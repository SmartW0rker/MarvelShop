@model IEnumerable<MarvelShop.Models.Kosnicka>

@{
    ViewBag.Title = "Kupi";
}

<h2>Kupi</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.broj_proizvodi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.status)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.embg)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.broj_proizvodi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.status)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.embg)
        </td>
       
    </tr>
}

</table>
