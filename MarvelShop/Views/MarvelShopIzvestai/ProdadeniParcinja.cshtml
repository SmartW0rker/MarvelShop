@model IEnumerable<MarvelShop.Models.ProdadeniProizvodi>
@{
    ViewBag.Title = "ProdadeniParcinja";
}

<h2>ProdadeniParcinja</h2>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ime_proizvod)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.kolicina)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.count)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ime_proizvod)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.kolicina)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.count)
            </td>
            
        </tr>
    }

</table>


